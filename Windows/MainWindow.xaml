<Window x:Class="cs_proj_ostateczny.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cs_proj_ostateczny"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="100,100,100,100">
        <StackPanel>
        <Button x:Name="PrzystankiButton"
                   Click="PrzystankiButton_Click">
            Przystanki
        </Button>
        <Button x:Name="KlienciButton"
                    Click="KlienciButton_Click">
            Klienci
        </Button>
        <Button x:Name="MotorniczyButton"
                   Click="MotorniczyButton_Click">
                Motorniczy
        </Button>
        <Button x:Name="Przystanki_na_trasieButton"
                   Click="Przystanki_na_trasieButton_Click">
                Przystanki na trasie
        </Button>
        <Button x:Name="TramwajeButton"
                   Click="TramwajeButton_Click">
                Tramwaje
        </Button>
        <Button x:Name="TrasyButton"
                   Click="TrasyButton_Click">
                Trasy
        </Button>
        </StackPanel>
    </Grid>
</Window>
